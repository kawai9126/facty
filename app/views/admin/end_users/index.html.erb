<h2 class='user_index'>会員一覧</h2>
<table class='user_index'>
    <tr>
        <th>会員ID</th>
        <th>ニックネーム</th>
        <th>氏名</th>
        <th>メールアドレス</th>
    </tr>
    <% @end_users.each do |end_user| %>
    <tr>
        <td><%= end_user.id %></td>
        <td><%= end_user.nick_name %></td> 
        <td><%= link_to end_user.user_name, admin_end_user_path(end_user) %></td>
        <td><%= end_user.email %></td>
  　</tr>
    <% end %>
    <%= paginate @end_users %>
</table>
