<h2>ジャンル編集</h2>
<div>
    <table>
        <%= form_with model: @genre ,url: admin_genre_path(@genre),local:true do |f| %>
        <tr>
            <td>ジャンル名</td>
            <td><%= f.text_field :name %></td>
            <td><%= f.submit '変更を保存', class: "btn btn-success" %></td>
        </tr>
        <% end %>
    </table>
</div>