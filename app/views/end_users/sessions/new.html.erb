<h2 class='session'>ログインページ</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="session">
    メールアドレス <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="session">
    パスワード <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="session">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="session">
    <%= f.submit "ログイン" %>
  </div>
<% end %>

<%#= render "end_users/shared/links" %>
