<h2>マイページ</h2>

    
    <div>
        登録情報<%= link_to "編集する", edit_public_end_user_path(@end_user), class: "btn btn-sm  btn-success" %>
    </div>
    
    <table>
        <tr>
            <th>ニックネーム</th>
            <td><%= @end_user.nick_name %></td>
          </tr>
    
        <tr>
            <th>名前</th>
            <td><%= @end_user.user_name %></td>
        </tr>
    
        <tr>
            <th>郵便番号</th>
            <td><%= @end_user.mail_number %></td>
        </tr>
    
        <tr>
            <th>住所</th>
            <td><%= @end_user.adress %></td>
        </tr>
    
        <tr>
            <th>電話番号</th>
            <td><%= @end_user.tel %></td>
        </tr>
    
        <tr>
            <th>メールアドレス</th>
            <td><%= @end_user.email %></td>
        </tr>
    </table>

<div>
    発送先
</div>

<div>
    注文履歴
</div>

</div>

